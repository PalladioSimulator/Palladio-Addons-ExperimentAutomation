<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.experimentautomation.application.feature"
      label="Experiment Automation Application Feature"
      version="5.3.0.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="https://sdqweb.ipd.kit.edu/wiki/Palladio_Experiment_Automation">
      The Palladio Experiment Automation enables automatic execution of Palladio simulation runs. Allows to specify experiment runs for arbitrary solvers within a model; also supports experiment variations, e.g., for sensitivity analyses.
   </description>

   <requires>
      <import plugin="de.uka.ipd.sdq.simucomframework" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.codegen.simucontroller" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.analyzer.workflow" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.simucomframework.simucomstatus" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.experimentautomation" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.dao" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.util" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.commons" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.jscience"/>
      <import plugin="org.palladiosimulator.recorderframework" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.simulizar.reconfiguration.storydiagram"/>
      <import plugin="org.palladiosimulator.servicelevelobjective.edp2" version="1.0.1" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.recorderframework.edp2" version="2.0.2" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.simulizar" version="2.0.3" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.datastream" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.measurementframework" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.repository.local" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.palladiosimulator.branding"/>
   </requires>

   <plugin
         id="org.palladiosimulator.experimentautomation.application"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
